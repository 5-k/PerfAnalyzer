{
    "manifestVersion": 1,
    "id": "jmeter-perf-analyzer",
    "name": "JMeter Performance Analyzer",
    "version": "1.1.49",
    "publisher": "id-az-pipeline",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "This task enables to run Performance testing using Apache Jmeter, Analyze report and post results. More details at https://github.com/5-k/PerfAnalyzer",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
         
        { "path": "perfanalyzer" }, 
        {
            "path": "overview.md",
            "addressable": true
        },
        {
            "path": "images/extension-icon.png",
            "addressable": true
        },
        {
            "path": "screenshots",
            "addressable": true
          }
    ],
    "screenshots": [
        { "path": "screenshots/P4.png"},
        { "path": "screenshots/P5.png"},
        { "path": "screenshots/P10.png"}
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "sample": {
            "path": "samples/README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "home": {
            "uri": "https://prateekmishra.me/"
        },
        "repository": {
            "uri": "https://github.com/5-k/PerfAnalyzer"
        },
        "issues": {
            "uri": "https://github.com/5-k/PerfAnalyzer/issues"
        },
        "linkedin": {
            "uri": "https://www.linkedin.com/in/prateek-mishra-61aa4658/"
        },
        "samples": {
            "uri" : "https://github.com/5-k/PerfAnalyzer/tree/main/samples/README.md"
        },
        "readMe": {
            "uri" : "https://github.com/5-k/PerfAnalyzer/blob/main/README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/5-k/PerfAnalyzer"
    },
    "branding": {
        "color": "#FFFFFF",
        "theme": "light"
    },
    "tags": [
        "performance", 
        "performance testing",
        "perf analyzer",
        "performanceanalyzer",
        "coverage",
        "publish",
        "report",
        "publishreport", 
        "SSDT",
        "Apache",
        "ApacheJMeter",
        "ApacheJMeterPerformanceTest",
        "coverage", 
        "tag"
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "perfanalyzer"
            }
        }
    ],
    "CustomerQnASupport": {
        "enablemarketplaceqna": true,
        "url": "http://uservoice.visualstudio.com"
    } 
}